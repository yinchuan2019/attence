<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.my.attence.mapper.AttRecordMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.my.attence.entity.AttRecord">
        <id column="id" property="id" />
        <result column="att_no" property="attNo" />
        <result column="att_name" property="attName" />
        <result column="att_begin_date" property="attBeginDate" />
        <result column="att_end_date" property="attEndDate" />
        <result column="att_type" property="attType" />
        <result column="att_remarks" property="attRemarks" />
        <result column="deleted" property="deleted" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, att_no, att_name, att_begin_date, att_end_date, att_type, att_remarks, deleted, create_time, update_time
    </sql>

</mapper>
